{
  "name": "govscode-flutter-dart",
  "displayName": "goVSCode Flutter & Dart",
  "description": "Dedicated to making our lives easier. Used by amazing Tap developers.üßû‚Äç",
  "version": "0.0.4",
  "publisher": "TapPayments",
  "repository": "https://github.com/Tap-Payments/goVSCODE",
  "engines": {
    "vscode": "^1.30.0"
  },
  "icon": "images/tap.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.supremeGetter"
  ],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "main-explorer",
          "title": "Tap Universe",
          "icon": "images/tap.svg"
        }
      ]
    },
    "views": {
      "main-explorer": [
        {
          "id": "commands",
          "name": "Control Tab"
        },
        {
          "id": "commands2",
          "name": "Others"
        },
        {
          "id": "commands3",
          "name": "Others"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "extension.supremeGetter",
          "group": "commands"
        },
        {
          "command": "extension.configBuilder",
          "group": "commands"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.supremeGetter",
        "title": "Get All",
        "category": "test",
        "icon": {
          "dark": "images/icons/dark/icon-tap.svg",
          "light": "images/icons/light/icon-tap.svg"
        }
      },
      {
        "command": "extension.configBuilder",
        "title": "Refresh",
        "category": "test",
        "icon": {
          "dark": "images/icons/dark/icon-refresh.svg",
          "light": "images/icons/light/icon-refresh.svg"
        }
      },
      {
        "command": "extension.configBuilder1",
        "title": "Refresh",
        "category": "test",
        "icon": {
          "dark": "images/icons/dark/icon-refresh.svg",
          "light": "images/icons/light/icon-refresh.svg"
        }
      }
    ]
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.30.0",
    "eslint": "^5.13.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.2"
  }
}
